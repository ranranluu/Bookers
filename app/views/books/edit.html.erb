<h1>Editing Book</h1>
<%= form_with model:@book,url:"/books/#{@book.id}", local:true do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :body %>
  <%= f.text_area :body %>

<%= f.submit 'Update Book' %>
<% end %>

<%= link_to "Edit", editbook_path(@book.id) %>
|
<%= link_to "Back", "/books" %>